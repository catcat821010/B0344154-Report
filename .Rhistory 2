education<-group_by(opendata105Y020,County) %>%
summarise(MS_MenOfCounty=sum(MS_Men),MS_WomenOfCounty=sum(MS_Women))
population<-group_by(opendata10512M030,County) %>%
summarise(Population_MenOfCounty=sum(Population_Men),Population_WomenOfCounty=sum(Population_Women))
library(dplyr)
MSDF<-inner_join(education,population,by=c("County"))
knitr::kable(MSDF)
MenMSRate<-MSDF$MS_MenOfCounty/MSDF$Population_MenOfCounty
WomenMSRate<-MSDF$MS_WomenOfCounty/MSDF$Population_WomenOfCounty
MSDF<-mutate(MSDF,MenMSRate,WomenMSRate,difference=MenMSRate-WomenMSRate)
View(MSDF)
MenMSRate<-round(MSDF$MS_MenOfCounty/MSDF$Population_MenOfCounty*100,2)
WomenMSRate<-round(MSDF$MS_WomenOfCounty/MSDF$Population_WomenOfCounty*100,2)
MSDF<-mutate(MSDF,MenMSRate,WomenMSRate,difference=MenMSRate-WomenMSRate)
MenMSRate<-round(MSDF$MS_MenOfCounty/MSDF$Population_MenOfCounty*100,2)
WomenMSRate<-round(MSDF$MS_WomenOfCounty/MSDF$Population_WomenOfCounty*100,2)
MSDF<-mutate(MSDF,MenMSRate,WomenMSRate,difference=MenMSRate-WomenMSRate)
View(MSDF)
